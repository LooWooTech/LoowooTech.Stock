      <Field Index="7" X="5" Y="6" Unit="平方米" Title="乡级规划-永久基本农田" SQL=""></Field>

      <Rule Left="6" Relation="大于等于"  Right="7"></Rule>
      <Rule Left="4" Relation="小于等于"  Right="2"></Rule>
    </Excel>
